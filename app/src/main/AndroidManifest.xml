<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asen.android.demo">

    <application
        android:name=".AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/demo_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.afindview.AFindViewActivity"
            android:label="@string/quickFindView" />
        <activity
            android:name=".ui.error.ErrorTestActivity"
            android:label="@string/testError" />
        <activity
            android:name=".ui.error.ErrorReadFileActivity"
            android:label="@string/testErrorFile" />
        <activity
            android:name=".ui.quickadapter.QuickAdapterActivity"
            android:label="@string/quickAdapter" />

    </application>

</manifest>